
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @*<link rel="stylesheet" href="~/Content/bootstrap-4.0/css/fonts/font-awesome.css">*@
    <link rel="stylesheet" href="~/Content/iconfont/iconfont.css">
    <link rel="stylesheet" href="~/Content/vxe-table/jsdelivr/style.css">   

    <!--begin 公共查詢引入Jquery樣式-->
    <link rel="stylesheet" type="text/css" href="~/Lib/jquery-easyui-1.5/themes/default/easyui.css" />
    <!--end-->
    @*<link rel="stylesheet" type="text/css" href="~/Lib/jquery-easyui-1.5/themes/icon.css" />*@
    <link rel="stylesheet" href="~/Content/ElementUI/css/element-ui.css" />   

    <style>
        .div-left {
            float: left;
            width: 80%;
            background-color: aqua;
            /*border:dotted;*/
        }

        .div-right {
            float: right;
            width: 16%;
            background-color: yellow;
        }

        .vxe-form-item-customize {
            /*width:25%;*/
            /*margin-top: -20px;*/
            height: 20px;
        }

        .input-customize {
            height: 20px;
            font-size: 10px;
            width: 25%;
        }

       .vxe-select > .vxe-input {
            width: 100%;
            height: 18px;
       }

       .vxe-input {
           height: 20px;
       }

       /* el-select
        * 自定义宽高樣式
       */
       .el-select {
           margin-top:0px;
           margin-bottom:1.5px;
       }
       .el-select .el-input__inner {
           /*width: 150px;*/
           font-size: 10px;
           height: 22px;
       }
       .el-select .el-input__suffix {
           top: 0px;           
       }
       .el-select .el-input__icon {
           line-height: inherit;
       }
       .el-select .el-input__suffix-inner {
           display: inline-block;
       }
       .el-select-dropdown__item:hover{
           color:#409eff;       
       }

    </style>

    @RenderSection("PageSpecificStyleSheetIncludes", required: false)
    <!--begin 公共查詢引入Jquery-->
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="~/Lib/jquery-easyui-1.5/jquery.easyui.min.js"></script>
    <!--end-->
   
    <script src="~/Content/vxe-table/js/vue.js"></script>
    <script src="~/Content/vxe-table/jsdelivr/xe-utils.js"></script>
    <script src="~/Content/vxe-table/jsdelivr/vxe-table.js"></script>
    <script type="text/javascript" src="~/Scripts/axios.js"></script>
    <script src="~/Content/Js/common.js"></script>
</head>
<body>
    @RenderBody()
    @RenderSection("PageSpecificJavascriptIncludes", required: false)   
</body>
</html>