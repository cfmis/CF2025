            </vxe-column>
            <vxe-column field="pbox_qty" title="數量/箱" width="120" :edit-render="{autofocus: '.vxe-input--inner'}">
                <template #edit="{ row }">
                    <vxe-input v-model="row.pbox_qty" style="height:20px;width:98%" @@change="setCurrentRowUpdateState(row)"></vxe-input>
