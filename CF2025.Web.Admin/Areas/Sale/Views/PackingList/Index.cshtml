                        <vxe-input v-model="data.create_by" class="input-customize" disabled></vxe-input>
                    </template>
                </vxe-form-item>
                <vxe-form-item title="建檔日期" field="create_date" :item-render="{}" span="6" style="margin-top:-18px" class="vxe-form-item-group">
